title コーチング || #{page.title}
meta(charset="utf-8")
meta(http-equiv="X-UA-Compatible" content="IE=edge")
meta(name="viewport", content="width=device-width, initial-scale=1.0")
meta(name="description" content=page.description)
meta(name="keywords" content=page.keywords)

meta(rel="shortcut icon" href="assets/img/favicon.ico")
meta(name="format-detection" content="telephone=no")
// ORG
meta(property="og:title", content=page.title)
meta(property="og:url", content="http://tkwebwd.xyz/coaching/")
meta(property="og:image", content="http://tkwebwd.xyz/coaching/assets/img/img-header-logo.png")
meta(property="og:title", content=sitename)
meta(property="og:description" content=sitedescripition)
meta(name="twitter:card", content="summary")
meta(name="twitter:site", content="コーチング")


block css
  link(href=page.dirPath + "assets/css/reset.css", rel="stylesheet", type="text/css")
  link(href=page.dirPath + "assets/css/style.css", rel="stylesheet", type="text/css")
  if page.pagecategory == 'contact'
    link(href="https://use.fontawesome.com/releases/v5.0.8/css/all.css", rel="stylesheet", type="text/css")

block hjs
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js", type="text/javascript")
  script(src=page.dirPath + "assets/js/jquerypjax.js", type="text/javascript")
  script(src=page.dirPath + "assets/js/main.js", type="text/javascript")
//-  if page.pagecategory == 'about'
    script(src=page.dirPath + "assets/js/bx.js", type="text/javascript")